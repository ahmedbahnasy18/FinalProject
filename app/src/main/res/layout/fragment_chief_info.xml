<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:clickable="true"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@android:color/darker_gray"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="11dp"
        android:id="@+id/cheifimage" />

    <TextView
        android:text="cheif name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/cheifimage"
        android:layout_toEndOf="@+id/cheifimage"
        android:layout_marginLeft="51dp"
        android:layout_marginStart="51dp"
        android:id="@+id/cheifname"
        android:layout_alignTop="@+id/cheifimage"
        android:textSize="18sp"
        android:textStyle="bold"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_star_big_off"
        android:id="@+id/star3"
        android:layout_alignTop="@+id/star2"
        android:layout_toEndOf="@+id/star2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_star_big_off"
        android:id="@+id/star4"
        android:layout_alignTop="@+id/star3"
        android:layout_toRightOf="@+id/star3"
        android:layout_toEndOf="@+id/star3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_star_big_off"
        android:id="@+id/star5"
        android:layout_alignTop="@+id/star4"
        android:layout_toRightOf="@+id/star4"
        android:layout_toEndOf="@+id/star4" />

    <TextView
        android:text="Opening Hours :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cheifimage"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="54dp"
        android:id="@+id/textView4" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:id="@+id/openinghours" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_star_big_off"
        android:id="@+id/star1"
        android:layout_alignTop="@+id/star2"
        android:layout_alignLeft="@+id/cheifname"
        android:layout_alignStart="@+id/cheifname" />

    <TextView
        android:text="Pick Up :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp"
        android:id="@+id/textView6" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pickupavailability"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignRight="@+id/star1"
        android:layout_alignEnd="@+id/star1" />

    <TextView
        android:text="Delivery :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="46dp"
        android:id="@+id/textView8" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignLeft="@+id/pickupavailability"
        android:layout_alignStart="@+id/pickupavailability"
        android:id="@+id/deliveryavailability" />

    <TextView
        android:text="Delivers in :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="47dp"
        android:id="@+id/textView10" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView10"
        android:layout_alignLeft="@+id/deliveryavailability"
        android:layout_alignStart="@+id/deliveryavailability"
        android:id="@+id/deliversin" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/btn_star_big_off"
        android:id="@+id/star2"
        android:layout_above="@+id/textView4"
        android:layout_toRightOf="@+id/star1"
        android:layout_toEndOf="@+id/star1" />


</RelativeLayout>
